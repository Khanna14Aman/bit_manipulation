#include<bits/stdc++.h>
using namespace std;
#define int long long
int brute(int n){
    int count =0;
    while(n!=0){
        if(n&1)count++;
        n=n>>1;
    }
    return count;
}
int brain(int n){
    int count=0;
    while(n!=0){
        count++;
        n = n&(n-1);
    }
    return count;
}
signed main(){
    int n;
    cin>>n;
    cout<<brute(n)<<endl;//log(n)==no of set+no of unset bits
    cout<<brain(n)<<endl;//tc==>no of set bits
    cout<<__builtin_popcount(n)<<endl;//log(n)
    return 0;
}
