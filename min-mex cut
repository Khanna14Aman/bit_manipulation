question==>https://codeforces.com/contest/1566/problem/B

#include<bits/stdc++.h>
using namespace std;
#define int long long 
signed main(){
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int count=0;
        int n = s.length();
        for(int i=0;i<n;i++){
            if(s[i]=='0'){
                count++;     // only need to consider 0 because minmex of 1 and 2 will always be 0
                if(s[i+1]=='0'){
                    count--;  // suppose there is consecutive zeros so we will take minmex 1 for all consecutive zeros
                }
            }
        }
        if(count==n){
            cout<<"1\n";
        }
        else if(count>2){
            cout<<"2\n";
        }
        else{
            cout<<count<<endl;
        }
    }
    return 0;
    
}
